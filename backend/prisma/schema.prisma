generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
  moduleFormat = "cjs"
}
  
datasource db {
  provider = "postgresql"
}

model User {
  id             Int        @id @default(autoincrement())
  firstName      String   
  surname        String
  email          String     @unique
  card           Int        @unique

}
